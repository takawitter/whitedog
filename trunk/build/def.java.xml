<?xml version="1.0" encoding="UTF-8" ?>
<project name="def.java">
	<presetdef name="javac5">
		<javac source="1.5" target="1.5"
			srcdir="${src}" destdir="${build}"
			encoding="UTF-8"
			classpathref="path.lib"
			/>
	</presetdef>

	<presetdef name="javac6">
		<javac source="1.6" target="1.6"
			srcdir="${src}" destdir="${build}"
			encoding="UTF-8"
			classpathref="path.lib"
			/>
	</presetdef>

	<presetdef name="jar">
		<jar
			basedir="${build}"
			destfile="${dist}/${jarfile}"
		/>
	</presetdef>
</project>
